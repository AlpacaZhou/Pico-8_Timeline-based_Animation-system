function update_lights()
 timer += 2
 if timer % 20 == 0 then
 
  if lmode == 4 then 
   for i, light in ipairs(lightset) do
    light.on = (i + flr(timer / 30)) % 2
   end
   
  elseif lmode == 2 then
   local toggle = (flr(timer / 30) % 2 == 0)
   for light in all(lightset) do
    light.on = toggle and 1 or 0
   end
  elseif lmode == 1 then 
   for light in all(lightset) do
    light.on = rnd(1) > 0.5 and 1 or 0
   end
  elseif lmode == 3 then 
  	local total_steps = #lightset * 2 
			local step = flr(timer / 10) % total_steps
 		for i, light in ipairs(lightset) do
    if step < #lightset then  
     light.on = (i <= step) and 1 or 0
    else  
     local dim_step = step - #lightset
     light.on = (i > dim_step) and 1 or 0
    end
 		end
  end
 end
 --switch modes
 if btnp(5) then
 	sfx(5)
 	lmode +=1
 	if lmode > 4 then
 		lmode = 1
 	end
 end
end

