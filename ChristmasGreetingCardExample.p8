-- create object
function crt(id,typa,x,y,w,h,c,v,l,s)
 local obj = {
 	id = id or #pkd+1,
 	--rec(rectangle) or spr(point)
 	t = typa,
		x = x, y = y, -- current pos
		x1 = x, y1 = y, -- start pos
		x2 = x, y2 = y, -- target pos
		w = w, h = h, 	-- size
		c = c, 		-- color
		v = v,			-- speed
		mov = lin,	 -- move curve
		layer = l or 1, --redenr sequence
		ospr = s or 0,		--sprite if typa = spr
		p = 0,			 -- progress
		}
  add(pkd, obj)
end
-- execute step (call wehn s)
function exe()
 for s in all(steps[si]) do
  local o = pkd[s[1]]
  o.x1, o.y1 = o.x, o.y
  o.x2, o.y2 = s[2]+o.x2, s[3]+o.y2
  o.mov = s[5]
  o.p = 0
  sfx(s[6])
  if s[4]>1 then--speed limit
  	s[4]=1
  end
  o.v=s[4]
 end
end

-- move object if not in position
function mov(o)
 o.p += o.v / 30
 if o.p > 1 then 
 	o.p = 1 
 end
 local t = o.mov(o.p)
 o.x = o.x1 + (o.x2 - o.x1) * t
 o.y = o.y1 + (o.y2 - o.y1) * t
 return o.p >= 1
end

-- update timeline
function upd_tm()
 if #tm == 0 then return end
 local cur = tm[1]
 if type(cur) == "number" then
  t += 1 / 30
  if t >= cur then
   t = 0
   deli(tm, 1)
  end
 elseif cur == "step" then
  exe()
  si += 1
  deli(tm, 1)
 end
end

-- function to check if all steps are finished
function all_steps_finished()
  --is timeline is empty
  if #tm > 0 then
   return false
  end
  --any object still moving
  for obj in all(pkd) do
   if obj.p < 1 then
    return false
   end
  end
  -- if both checks pass
  return true
end

