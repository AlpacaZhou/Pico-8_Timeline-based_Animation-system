function update_rows()   
 for row in all(rows) do
  if row.growing then
   row.size += growth_speed
   if row.size >= max_size then
 			row.growing = false
   end
  else
   row.size -= shrink_speed
   if row.size <= 0 then
    del(rows, row)
   end
  end
 end
 add_timer += 1 / 30 
 if add_timer >= add_interval and #rows * row_spacing < 128 + max_size then
  local new_row = 
  {
   y = #rows * row_spacing, 
   size = 1, 
   growing = true 
  }
  add(rows, new_row)
  gen += 1
  add_timer = 0 
 end
 --next mode
 if gen == 17 then
 	mode = "packed"
 end
 --stop
 if gen > 33 then
  animation_done = true
 end
end
